; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="NSMB Minigames"
run/main_scene="res://menus/copyright_notice.tscn"
config/features=PackedStringArray("4.0", "Mobile")
config/icon="res://icon.png"

[autoload]

Transitionizer="*res://globals/transitionizer.tscn"
DBs="*res://globals/DBs.gd"

[display]

window/size/viewport_width=720
window/size/viewport_height=720
window/handheld/orientation=5
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg")

[gui]

theme/custom="res://menus/assets/theme_resources/plain.tres"

[rendering]

renderer/rendering_method="mobile"
environment/defaults/default_clear_color=Color(0, 0, 0, 1)
